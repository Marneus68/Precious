{
	"$schema": "vscode://schemas/color-theme",
  "name": "Precious Dark",
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#f3f8fa"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#49176d"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#12bd77"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": {
        "foreground": "#fff8ab"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#654da0"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#fff8ab"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f3f8fa"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#fff8ab"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#ff7272"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#78fdc6"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#ff7272"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#12bd77"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#49176d"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#ff7272"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#4d4d4d"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#49176d"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ff7272"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#78fdc6"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#1e64a1"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#1e64a1"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#1e64a1"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "keyword.operator.noexcept"
      ],
      "settings": {
        "foreground": "#1e64a1"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#78fdc6"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#78fdc6"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#78fdc6"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#c33a80"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#49176d"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#adb8bf"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#654da0"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#654da0"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#f3f8fa"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#654da0"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#1e64a1"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#654da0"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#659ccd"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#78fdc6"
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "newOperator":"#654da0",
    "stringLiteral":"#c33a80",
    "customLiteral": "#fff8ab",
    "numberLiteral": "#78fdc6",
  },
  "colors": {
    "focusBorder": "#00000000",
    "foreground": "#f3f8fa",
    "selection.background": "#49176d",
    "errorForeground": "#ff7272",
    "widget.shadow": "#00000000",

    "window.activeBorder": "#49176d",
    "window.inactiveBorder": "#4d4d4d",

    "textLink.activeForeground": "#1e64a1",
    "textLink.foreground": "#659ccd",
    "textPreformat.foreground": "#f3f8fa",
    "textSeparator.foreground": "#4d4d4d",

    "toolbar.hoverBackground": "#101010",

    "button.background": "#101010",
    "button.foreground": "#f3f8fa",
    "button.border": "#4d4d4d",
    "button.hoverBackground": "#49176d",
    "button.secondaryBackground": "#101010",
    "button.secondaryForeground": "#f3f8fa",
    "button.secondaryHoverBackground": "#49176d",
    "checkbox.background": "#101010",
    "checkbox.foreground": "#f3f8fa",
    "checkbox.border": "#4d4d4d",

    "dropdown.background": "#101010",
    "dropdown.listBackground": "#202020",
    "dropdown.border": "#49176d",
    "dropdown.foreground": "#f3f8fa",

    "badge.background": "#81293e",
    "badge.foreground": "#f3f8fa",

    "progressBar.background": "#81293e",

    "activityBar.background": "#101010",
    "activityBar.foreground": "#f3f8fa",
    "activityBar.dropBorder": "#4d4d4d",
    "activityBar.inactiveForeground": "#4d4d4d",
    "activityBarBadge.background": "#81293e",
    "activityBarBadge.foreground": "#f3f8fa",
    "activityBar.activeBorder": "#49176d",
    "activityBar.activeFocusBorder": "#49176d",
    "activityBar.activeBackground": "#49176d",

    "sideBar.background": "#101010",
    "sideBar.foreground": "#f3f8fa",
    "sideBarTitle.foreground": "#f3f8fa",
    "sideBarSectionHeader.background": "#101010",
    "sideBarSectionHeader.foreground": "#f3f8fa",

    "editorGroupHeader.noTabsBackground": "#101010",
    "editorGroupHeader.tabsBackground": "#101010",
    "editorGroup.emptyBackground": "#101010",
    "tab.activeBackground": "#49176d",
    "tab.activeForeground": "#f3f8fa",
    "tab.unfocusedActiveBackground": "#4d4d4d",
    "tab.unfocusedActiveForeground": "#adb8bf",
    "tab.border": "#00000000",
    "sideBySideEditor.border": "#00000000",
    "editorGroup.border": "#00000000",

    "editor.background": "#101010",
    "editor.foreground": "#adb8bf",

    "panel.background": "#101010",
    "panel.border": "#4d4d4d",

    "statusBar.background": "#49176d",
    "statusBar.foreground": "#f3f8fa",
    //"statusBar.border": "#49176d",
    "statusBar.debuggingBackground": "#81293e",
    "statusBar.debuggingForeground": "#f3f8fa",
    //"statusBar.debuggingBorder": "#81293e",
    "statusBar.noFolderBackground": "#4d4d4d",
    "statusBar.noFolderForeground": "#f3f8fa",
    //"statusBar.noFolderBorder": "#4d4d4d" ,
    "statusBarItem.prominentBackground": "#654da0",
    "statusBarItem.prominentForeground": "#ffffff",
    "statusBarItem.errorBackground": "#dc3535",
    "statusBarItem.errorForeground": "#f3f8fa",
    "statusBarItem.warningBackground": "#d2c533",
    "statusBarItem.warningForeground": "#f3f8fa",
    "statusBarItem.remoteBackground": "#81293e",
    "statusBarItem.remoteForeground": "#f3f8fa",

    "titleBar.activeBackground": "#101010",
    "titleBar.activeForeground": "#f3f8fa",
    "titleBar.inactiveBackground": "#101010",
    "titleBar.inactiveForeground": "#adb8bf",

    "menubar.selectionBackground": "#4d4d4d",
    "menubar.selectionForeground": "#ffffff",
    "menubar.selectionBorder": "#00000000",
    "menu.background": "#101010",
    "menu.foreground": "#f3f8fa",
    "menu.selectionBackground": "#4d4d4d",
    "menu.selectionForeground": "#ffffff",
    "menu.separatorBackground": "#4d4d4d",
    "menu.border": "#654da0",

    "notificationCenter.border": "#49176d",
    "notificationCenterHeader.background": "#202020",
    "notificationCenterHeader.foreground": "#f3f8fa",
    "notificationToast.border": "#49176d",
    "notifications.background": "#101010",
    "notifications.foreground": "#f3f8fa",
    "notifications.border": "#49176d",
    "notificationLink.foreground": "#659ccd",
    "notificationsErrorIcon.foreground": "#dc3535",
    "notificationsWarningIcon.foreground": "#d2c533",
    "notificationsInfoIcon.foreground": "#1e64a1",

    "terminal.background": "#101010",
    "terminal.foreground": "#f0f0f0",
    "terminal.ansiBlack": "#202020",
    "terminal.ansiBrightBlack": "#4d4d4d",
    "terminal.ansiRed": "#dc3535",
    "terminal.ansiBrightRed": "#ff7272",
    "terminal.ansiGreen": "#12bd77",
    "terminal.ansiBrightGreen": "#78fdc6",
    "terminal.ansiYellow": "#d2c533",
    "terminal.ansiBrightYellow": "#fff8ab",
    "terminal.ansiBlue": "#49176d",
    "terminal.ansiBrightBlue": "#654da0",
    "terminal.ansiMagenta": "#81293e",
    "terminal.ansiBrightMagenta": "#c33a80",
    "terminal.ansiCyan": "#1e64a1",
    "terminal.ansiBrightCyan": "#659ccd",
    "terminal.ansiWhite": "#adb8bf",
    "terminal.ansiBrightWhite": "#f3f8fa",

    "welcomePage.tileBackground": "#202020",
    "welcomePage.tileHoverBackground": "#202020",
    "welcomePage.progress.background": "#000000",
    "welcomePage.progress.foreground": "#81293e",
    "welcomePage.tileShadow.": "#00000000",
  }
}