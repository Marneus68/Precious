{
  "name": "Precious",
  "semanticHighlighting": true,
  "tokenColors" : [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": { "foreground": "#12bd77" }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": { "foreground": "#ff7272" }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": { "foreground": "#fff8ab" }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": { "foreground": "#c33a80" }
    },
    {
      "name": "User-defined constant",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#c33a80" }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": { "foreground": "#654da0" }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": { "foreground": "#78fdc6" }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": { "fontStyle": "", "foreground": "#dc3535" }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": { "foreground": "#ff7272" }
    },
  ],
  "colors": {
